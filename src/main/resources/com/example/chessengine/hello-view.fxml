<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.chessengine.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <StackPane  fx:id="BoardContainer"
                VBox.vgrow="ALWAYS"
                maxWidth="Infinity"
                maxHeight="Infinity">
        <GridPane fx:id="ChessGrid"/>
    </StackPane>
</VBox>
